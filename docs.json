{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Stable Sea Terminal API",
  "colors": {
    "primary": "#3b82f6",
    "light": "#3b82f6",
    "dark": "#3b82f6"
  },
  "favicon": {
    "light": "/favicon-light.png",
    "dark": "/favicon-dark.png"
  },
  "api": {
    "baseUrl": "https://sandbox.stablesea.com/terminal/v1",
    "auth": {
      "method": "bearer",
      "default": "DEFAULT-TOKEN-HERE"
    },
    "playground": {
      "mode": "show"
    },
    "mdx": {
      "server": "https://sandbox.stablesea.com/terminal/v1",
      "auth": {
        "method": "bearer",
        "default": "DEFAULT-TOKEN-HERE"
      }
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "API reference",
        "openapi": "api-reference/openapi.json",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api-reference/overview/authentication",
              "api-reference/overview/making-requests",
              "api-reference/overview/error-handling",
              "api-reference/overview/idempotency",
              "api-reference/overview/reliability-compliance"
            ]
          },
          {
            "group": "Resources",
            "pages": [
              "api-reference/resources/customer",
              "api-reference/resources/external-payment-instrument",
              "api-reference/resources/offering",
              "api-reference/resources/quote",
              "api-reference/resources/order",
              "api-reference/resources/hours-of-operation",
              "api-reference/resources/enumerations"
            ]
          }
        ]
      },
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Guides",
            "pages": [
              "guides/order-flow-walkthrough",
              "guides/failed-states"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/logo-lightmode.png",
    "dark": "/logo/logo-darkmode.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@stablesea.xyz"
      }
    ]
  },
  "contextual": {
    "options": [
      "copy"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/stablesea_xyz",
      "linkedin": "https://www.linkedin.com/company/stablesea/?viewAsMember=true"
    }
